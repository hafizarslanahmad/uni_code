<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Country Name</th>
      <th scope="col">User Admin</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @hecs.each do |hec|%>
       <tr>
        <th scope="row"><%= hec.id%></th>
        <td><%= hec.country%></td>
        <td><%= hec.user.email%></td>
        <td><%= link_to "edit", edit_hec_path(hec.id)%></td>
        <td><%= link_to "show", hec_path(hec.id)%></td>
        <td><%= button_to "delete", hec_path(hec.id), method: :delete %></td>
      </tr>
    <%end%>
  </tbody>
</table>
