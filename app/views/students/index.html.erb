<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Student Name</th>
      <th scope="col">Student Program</th>
      <th scope="col">Student F_name</th>
      <th scope="col">University Id</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student|%>
       <tr>
        <th scope="row"><%= student.id%></th>
        <td><%= student.name%></td>
        <td><%= student.program%></td>
        <td><%= student.f_name%></td>
        <td><%= student.university_id%></td>
        <td><%= link_to "edit", edit_student_path(student.id)%></td>
        <td><%= link_to "show", student_path(student.id)%></td>
        <td><%= button_to "delete", student_path(student.id), method: :delete %></td>
      </tr>
    <%end%>
  </tbody>
</table>
